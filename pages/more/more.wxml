<view class='container'>
  <scroll-view scroll-y="true">
    <view class='img_wrap' wx:for="{{advers}}" wx:key="{{index}}" 
          bindtap='jump' data-appid="{{item.appid}}" data-url="{{item.url}}">
      <image src='{{item.ad_url}}' mode='aspectFill'></image>
    </view>    
  </scroll-view>
</view>
